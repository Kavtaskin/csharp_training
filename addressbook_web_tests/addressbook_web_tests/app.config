<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="linq2db" type="LinqToDB.Configuration.LinqToDBSection, linq2db" requirePermission="false" />
  </configSections>

  <linq2db defaultConfiguration="SQLiteInDir" />

  <connectionStrings>
    <add name="AddressBook"
         connectionString="Server=localhost;Port=3306;Database=addressbook;Uid=root;Pwd=;charset=utf8;"
         providerName="MySql.Data.MySqlClient" />
    <!--Place your ConnectionString and ProviderName here-->
  </connectionStrings>
</configuration>